# CDC Attendance Management System - Frontend Environment Configuration

# API Configuration
# Development API URL (when running backend locally)
VITE_DEV_API_URL=http://localhost:5000/api

# Local API URL (override for local development)
VITE_LOCAL_API_URL=http://localhost:5000/api

# Production API URL
VITE_API_URL=https://your-backend-domain.com/api

# App Configuration
VITE_APP_NAME=CDC Attendance Management
VITE_APP_VERSION=1.0.0

# Development Configuration
VITE_USE_POLLING=false

# Build Configuration
ANALYZE=false

# Feature Flags
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PWA=false
VITE_ENABLE_ANALYTICS=false

# Debug Configuration
VITE_DEBUG=false
VITE_VERBOSE_LOGGING=false
